{
  "name": "sensors",
  "version": "1.0.0",
  "title": "Environment Sensors UI",
  "description": "Environment Sensors UI for IoT sensor data visualisation",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "update-types": "flow-typed update --ignoreDeps peer dev bundled -s",
    "clean:build": "rm -rf ../server/build/ui",
    "dev:build": "npm run clean:build && webpack --watch --config development.webpack.js",
    "prebuild": "npm run clean:build && npm run relay",
    "build": "webpack --config production.webpack.js",
    "relay": "relay-compiler --src ./src --schema ./src/graphql/schema.graphql",
    "relay:watch": "npm run relay -- --watch",
    "lint": "eslint --fix src/**/*.js",
    "format": "prettier --write src/**/*.js"
  },
  "keywords": [],
  "author": "lukasz@szmit.eu",
  "license": "MIT",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "@workday/canvas-colors-web": "^2.0.0",
    "classnames": "^2.2.5",
    "core-js": "^3.8.2",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.12",
    "highcharts": "^8.2.2",
    "highcharts-react-official": "^3.0.0",
    "history": "^5.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-relay": "0.0.0-experimental-0b967d2e",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.6.2",
    "regenerator-runtime": "^0.13.7",
    "relay-runtime": "^10.1.2",
    "typeface-roboto": "^1.1.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-json-strings": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "^7.12.10",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-relay": "^10.1.2",
    "css-loader": "^3.6.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-relay": "^1.8.1",
    "file-loader": "^6.2.0",
    "flow-bin": "^0.142.0",
    "flow-typed": "^3.2.1",
    "graphql": "^15.4.0",
    "html-webpack-plugin": "^4.5.1",
    "import-glob": "^1.5.0",
    "imports-loader": "^1.2.0",
    "mini-css-extract-plugin": "^1.3.3",
    "null-loader": "^4.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.2.1",
    "relay-compiler": "^10.1.2",
    "script-loader": "^0.7.2",
    "terser": "^5.5.1",
    "terser-webpack-plugin": "^4.0.0",
    "webpack": "^4.45.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^3.3.12"
  },
  "optionalDependencies": {
    "fsevents": "*",
    "inotify": "*"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "parser": "babel-flow",
    "printWidth": 100,
    "quoteProps": "as-needed",
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "tabWidth": 2
  }
}
